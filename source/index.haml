---
title: myokoym.net
---

%h1 myokoym.net

#main
  %ul.tabs
    %li.tab
      %input#tab1(type="radio" checked="checked" name="tabs")
      %label(for="tab1") Social
      #tab-content1.tab-content
        %ul.tabs
          - data.social.websites.each_with_index do |website, i|
            - id = "social-website-#{i}"
            %li
              %a{href: website.uri, id: id}
                %label(for="#{id}")= website.name
    %li.tab
      %input#tab2(type="radio" name="tabs")
      %label(for="tab2") Application
      #tab-content2.tab-content
        %ul.tabs
          - data.software.applications.each_with_index do |application, i|
            - id = "software-application-#{i}"
            %li
              %a(href="/#{application.uri}/" id="#{id}")
                %label(for="#{id}")= "#{application.name} - #{application.summary}"
    %li.tab
      %input#tab3(type="radio" name="tabs")
      %label(for="tab3") Profile
      #tab-content3.tab-content
        %ul
          - data.profile.titles.each do |title|
            %li= title
    %li.tab
      %input#tab4(type="radio" name="tabs")
      %label(for="tab4") Power
      #tab-content4.tab-content
        %ul.tabs
          - data.powered.websites.each_with_index do |website, i|
            - id = "powered-website-#{i}"
            %li
              %a{href: website.uri, id: id}
                %label(for="#{id}")= website.name
    %li.tab
      %input#tab5(type="radio" name="tabs")
      %label(for="tab5") Other
      #tab-content5.tab-content
        Welcome to my website :-)
%footer
  %p (c) 2011-2013 Masafumi Yokoyama
